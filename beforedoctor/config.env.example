# BeforeDoctor API Configuration
# Copy this file to .env and fill in your actual API keys

# Voice APIs
XAI_GROK_API_KEY=<XAi kEY>
OPENAI_API_KEY=<oPEN ai Keys>

# Weather API
OPENWEATHER_API_KEY=your_openweather_api_key_here

# ML Kit API
ML_KIT_API_KEY=ML_KIT_API_KEY

# Firebase Configuration
FIREBASE_PROJECT_ID=beforedoctor-7666f
FIREBASE_API_KEY=your_firebase_api_key

# App Configuration
APP_NAME=BeforeDoctor
APP_VERSION=1.0.0
ENVIRONMENT=development

# Voice API Settings (Default)
PRIMARY_VOICE_API=grok
FALLBACK_VOICE_API=openai
AUTO_FALLBACK_ENABLED=true

# Language Settings
DEFAULT_LANGUAGE=en
SUPPORTED_LANGUAGES=en,es,zh,fr,hi

# Database Configuration
SQLITE_DATABASE_NAME=beforedoctor.db
SQLITE_VERSION=1

# Testing Configuration
TEST_MODE=false
SYNTHETIC_LOGS_COUNT=100 